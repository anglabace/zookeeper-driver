<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Spring Fox 配置 -->
    <bean name="applicationSwaggerConfig" class="org.sagesource.zookeeperdriver.web.springfox.SpringFoxConfig">
        <property name="title" value="${springfox.title}"/>
        <property name="description" value="${springfox.description}"/>
        <property name="version" value="${springfox.version}"/>
        <property name="contactEmail" value="${springfox.contact.email}"/>
        <property name="contactName" value="${springfox.contact.name}"/>
        <property name="contactUrl" value="${springfox.contact.utl}"/>
    </bean>

</beans>